<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_height="match_parent"
        android:layout_width="match_parent">

    <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="10dp"
            tools:context=".MainActivity">

        <Button
                android:layout_gravity="center"
                android:onClick="dataBinding"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="DataBinding"/>
        <Button
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="liveData"
                android:text="LiveData"/>
        <Button
                android:layout_gravity="center"
                android:onClick="lifeCycle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="LifeCycle"/>
        <Button
                android:layout_gravity="center"
                android:onClick="room"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Room"/>
        <Button
                android:layout_gravity="center"
                android:onClick="navi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Navigation"/>
        <Button
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="paging"
                android:text="Paging"/>
        <Button
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="work"
                android:text="Work"/>
        <Button
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:onClick="rxjava"
                android:layout_height="wrap_content"
                android:text="RxJava"/>
        <Button
                android:layout_gravity="center"
                android:onClick="kotlin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kotlin"/>
        <Button
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:onClick="aac"
                android:layout_height="wrap_content"
                android:text="AAC"/>
        <Button
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:onClick="dagger"
                android:layout_height="wrap_content"
                android:text="Dagger"/>


    </LinearLayout>
</androidx.core.widget.NestedScrollView>
