<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		package="org.zhiwei.jetpack">

	<application
			android:allowBackup="true"
			android:icon="@mipmap/ic_launcher"
			android:label="@string/app_name"
			android:supportsRtl="true"
			android:theme="@style/AppTheme">
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<!--用于android link的跳转用的，需要配合scheme的使用-->
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
			</intent-filter>
		</activity>
		<activity
				android:name=".databinding.DBActivity"
				android:label="DataBindingActivity" />
		<activity
				android:name=".livedata.LiveDataActivity"
				android:label="LiveDataActivity" />
		<activity
				android:name=".lifecycle.LifeActivity"
				android:label="LifeActivity" />
		<activity
				android:name=".room.RoomActivity"
				android:label="RoomActivity" />
		<activity
				android:name=".navigation.NaviActivity"
				android:label="NaviActivity" />
		<activity
				android:name=".paging.PagingActivity"
				android:label="PagingActivity" />
		<activity
				android:name=".workmanager.WorkActivity"
				android:label="WorkActivity" />
		<activity
				android:name=".aac.view.AacActivity"
				android:label="AacActivity" />
		<activity
				android:name=".dagger.DaggerActivity"
				android:label="DaggerActivity" />
		<activity
				android:name=".kotlin.KotlinActivity"
				android:label="KotlinActivity" />
		<activity
				android:name=".rxjava.RxActivity"
				android:label="RxActivity" />
	</application>

</manifest>