<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.zhiwei.jetpack">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<!--用于android link的跳转用的，需要配合scheme的使用-->
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
			</intent-filter>
		</activity>
		<activity
			android:name=".live.temp.LiveDataActivity"
			android:label="LiveDataActivity" />
		<activity
			android:name=".life.temp.LifeActivity"
			android:label="LifeActivity" />
		<activity
			android:name=".room.RoomActivity"
			android:label="RoomActivity" />
		<activity
			android:name=".nav.NaviActivity"
			android:label="NaviActivity" />
		<activity
			android:name=".page.PagingActivity"
			android:label="PagingActivity" />
		<activity
			android:name=".work.temp.WorkActivity"
			android:label="WorkActivity" />
		<activity
			android:name=".aac.view.AacActivity"
			android:label="AacActivity" />
		<activity
			android:name=".dagger2.temp.DaggerActivity"
			android:label="DaggerActivity" />
		<activity
			android:name=".kt.KtActivity"
			android:label="KotlinActivity" />
		<activity
			android:name=".rxjava.RxActivity"
			android:label="RxActivity" />
	</application>

</manifest>